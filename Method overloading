class calculator{
    public  int add(int n1,int n2)
    {
      return n1+n2;
    }
    public int add(int n1,int n2,int n3)
    {
        return n1+n2+n3;
    }
}
class method{
    public static void main(String[] args) {
        calculator obj=new calculator();
        int result=obj.add(9, 7,8);
        System.out.println(result);
    }
}
